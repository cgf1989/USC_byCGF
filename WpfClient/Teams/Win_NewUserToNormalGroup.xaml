<Window x:Class="WpfClient.Teams.Win_NewUserToNormalGroup"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:WpfClient.Teams"
        mc:Ignorable="d"
        Title="添加群成员" Height="300" Width="200" Loaded="Window_Loaded">
    <Grid>
        <Grid.RowDefinitions>
            <RowDefinition Height="30"/>
            <RowDefinition/>
            <RowDefinition Height="50"/>
        </Grid.RowDefinitions>
        <Label Content="选择联系人"/>
        <ListBox Name="lbox_Contacts" Grid.Row="1"/>
        <Button Name="btn_AddToGroup" Grid.Row="2" HorizontalAlignment="Right" Margin="10" Content="添加" Width="80" Height="30" Click="btn_AddToGroup_Click"/>
    </Grid>
</Window>
